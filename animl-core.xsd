	<!-- -->
	<!--AttributeGroup for sparse data-->
	<xsd:attributeGroup name="ValueSet">
		<xsd:annotation>
			<xsd:documentation>Start and End Index for this Set of Values (zero-based).</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="startIndex" type="xsd:nonNegativeInteger" use="optional">
			<xsd:annotation>
				<xsd:documentation>Zero-based index where this Value Set starts. The specification is inclusive.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="endIndex" type="xsd:nonNegativeInteger" use="optional">
			<xsd:annotation>
				<xsd:documentation>Zero-based Index of the last Entry in this Value Set. The specification is inclusive.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:attributeGroup>
	<!---->
	<!--Stuff for digital signatures and audit trail-->
	<xsd:attributeGroup name="SignableItem">
		<xsd:annotation>
			<xsd:documentation>ID for Signable Items.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="id" type="xsd:ID" use="optional">
			<xsd:annotation>
				<xsd:documentation>Anchor point for digital signature. This identifier is referred to from the "Reference" element in a Signature. Unique per document.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:attributeGroup>
	<xsd:attributeGroup name="SignableItemWithName">
		<xsd:annotation>
			<xsd:documentation>ID and Name attribute for Signable Items.</xsd:documentation>
		</xsd:annotation>
		<xsd:attributeGroup ref="SignableItem"/>
		<xsd:attribute name="name" type="xsd:token" use="required">
			<xsd:annotation>
				<xsd:documentation>Plain-text name of this item.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:attributeGroup>
	<!---->
	<!--Things used for multiple purposes-->
	<xsd:simpleType name="UserTypeType">
		<xsd:annotation>
			<xsd:documentation>Specifies whether a user is a real person, a device, or a software program.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="human">
				<xsd:annotation>
					<xsd:documentation>Specifies that the user is a real person.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="device">
				<xsd:annotation>
					<xsd:documentation>Specifies that the user is a device.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="software">
				<xsd:annotation>
					<xsd:documentation>Specifies that the user is a software system.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!--
THE ELEMENT IS NOT USED. 
	<xsd:element name="DataPointReference" type="DataPointReferenceType">
		<xsd:annotation>
			<xsd:documentation>Series and Value in Super-Page being referenced.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
-->
	<xsd:complexType name="DataPointReferenceType">
		<xsd:annotation>
			<xsd:documentation>Basis type for a reference to a data point or value range.</xsd:documentation>
		</xsd:annotation>
		<xsd:attributeGroup ref="SignableItemWithName"/>
		<xsd:attribute name="seriesID" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>Contains the ID of the Series referenced.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="startIndex" type="xsd:nonNegativeInteger" use="required">
			<xsd:annotation>
				<xsd:documentation>Specifies the start value of the Series referenced. It can be either a Series index or a value within the value range of the Series specified.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="endIndex" type="xsd:nonNegativeInteger" use="optional">
			<xsd:annotation>
				<xsd:documentation>Specifies how many values, starting from the startValue, should be referenced. Used to refer to an area, rather than an individual value.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:element name="SIUnit" type="SIUnitType">
		<xsd:annotation>
			<xsd:documentation>Combination of SI units used to represent measurement unit</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="SIUnitType">
		<xsd:annotation>
			<xsd:documentation>Combination of SI units used to represent measurement unit</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="SIUnitNameList">
				<xsd:attribute name="factor" type="xsd:float" use="optional" default="1"/>
				<xsd:attribute name="exponent" type="xsd:float" use="optional" default="1"/>
				<xsd:attribute name="offset" type="xsd:float" use="optional" default="0"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="LabelType">
		<xsd:annotation>
			<xsd:documentation>Contains a name/symbol to discribe a scientific unit of measure.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!---->
	<!--Tracking Tag Elements and Types-->
	<xsd:element name="TrackingTagSet" type="TrackingTagSetType">
		<xsd:annotation>
			<xsd:documentation>Set of Tracking Tags</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="TrackingTagSetType">
		<xsd:annotation>
			<xsd:documentation>Set of Tracking Tags</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="TrackingTag" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="TrackingTag" type="TrackingTagType">
		<xsd:annotation>
			<xsd:documentation>Tag to mark related data items. When a value is given, it may also serve as a reference to an external data system.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="TrackingTagType">
		<xsd:annotation>
			<xsd:documentation>Tag to mark related data items. When a value is given, it may also serve as a reference to an external data system.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="name" type="xsd:token" use="required"/>
		<xsd:attribute name="value" type="xsd:string" use="optional"/>
	</xsd:complexType>
	<xsd:element name="Manufacturer" type="xsd:token">
		<xsd:annotation>
			<xsd:documentation>Company name.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Version">
		<xsd:annotation>
			<xsd:documentation>Version identifier of software release.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="SoftwareType">
		<xsd:sequence>
			<xsd:element ref="Manufacturer" minOccurs="0"/>
			<xsd:element ref="Name"/>
			<xsd:element ref="Version" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="Software" type="SoftwareType">
		<xsd:annotation>
			<xsd:documentation>Software used to create this.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="DeviceType">
		<xsd:sequence>
			<xsd:element ref="Manufacturer"/>
			<xsd:element ref="Name"/>
			<xsd:element ref="FirmwareVersion" minOccurs="0"/>
			<xsd:element ref="SerialNumber" minOccurs="0"/>
			<xsd:element ref="TrackingTagSet" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="FirmwareVersion" type="xsd:token">
		<xsd:annotation>
			<xsd:documentation>Version identifier of firmware software release</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SerialNumber" type="xsd:token">
		<xsd:annotation>
			<xsd:documentation>Unique serial number of device.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Device" type="DeviceType"/>
</xsd:schema>
