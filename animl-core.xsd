	<xsd:complexType name="SingleValueType">
		<xsd:annotation>
			<xsd:documentation>Elements for IndividualValueSets in Series.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="UnboundedValueType">
				<xsd:choice>
					<xsd:element ref="Int"/>
					<xsd:element ref="Long"/>
					<xsd:element ref="Float32"/>
					<xsd:element ref="Float64"/>
					<xsd:element ref="String"/>
					<xsd:element ref="Boolean"/>
					<xsd:element ref="DateTime"/>
					<xsd:element ref="PNG"/>
					<xsd:element ref="EmbeddedXML"/>
					<xsd:element ref="SVG"/>
				</xsd:choice>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="NumericValueType">
		<xsd:annotation>
			<xsd:documentation>Elements for numeric Series values.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="UnboundedValueType">
				<xsd:choice>
					<xsd:element ref="Int"/>
					<xsd:element ref="Long"/>
					<xsd:element ref="Float32"/>
					<xsd:element ref="Float64"/>
				</xsd:choice>
			</xsd:restriction>
